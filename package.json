{
  "name": "gcp-authcalator",
  "version": "0.1.4",
  "private": true,
  "license": "MIT",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "start": "bun run index.ts",
    "test": "bun test",
    "typecheck": "bun run tsc --noEmit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "check": "bun run lint && bun run format:check",
    "build": "bun run scripts/build.ts",
    "build:linux-amd64": "bun run scripts/build.ts --target=bun-linux-x64 --outfile=gcp-authcalator-linux-amd64",
    "build:darwin-arm64": "bun run scripts/build.ts --target=bun-darwin-arm64 --outfile=gcp-authcalator-darwin-arm64",
    "release": "bun run scripts/release.ts",
    "prepare": "prek install"
  },
  "dependencies": {
    "google-auth-library": "^10.5.0",
    "smol-toml": "^1.6.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
